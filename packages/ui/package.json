{
  "name": "@ssuperlilei/ui",
  "description": "@ssuperlilei ui library",
  "version": "3.1.2",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "homepage": "https://ssuperlilei.github.io/webTemplate/",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rm -rf dist && vite build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "publish": "pnpm publish"
  },
  "devDependencies": {
    "@ssuperlilei/i18n": "workspace:*",
    "@ssuperlilei/typescript-config": "workspace:*",
    "@ssuperlilei/utils": "workspace:*",
    "@ssuperlilei/hooks": "workspace:*",
    "@types/node": "^22.13.8",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "sass": "^1.85.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.1",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@css-render/plugin-bem": "^0.15.14",
    "css-render": "^0.15.14"
  }
}
